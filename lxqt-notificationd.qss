Notification {
    color: #D2D8E0;
    border: 0px solid rgb(0, 0, 0);
    background: rgba(0, 0, 0, 80%);
    margin: 0px;
}

QLabel {
    color: #D2D8E0;
}

#closeButton {
    border: none;
    padding: 2px;
    qproperty-icon: url(lxqt-notificationd/close.svg);
}

#closeButton:hover {
    qproperty-icon: url(lxqt-notificationd/close-hover.png);
}

#appLabel {
    font-weight: bold;
}
